    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^29.1.2",
